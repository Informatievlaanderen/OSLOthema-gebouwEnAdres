@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<https://data.vlaanderen.be/id/conceptscheme/adresstatus>
    a skos:ConceptScheme ;
    skos:prefLabel "conceptscheme adresstatus" ;
    skos:definition "Codelijst voor de status van een adres." .

<https://data.vlaanderen.be/id/concept/adresstatus/gehistoreerd>
    a skos:Concept ;
    skos:definition "Het adres werd formeel opgeheven door de gemeenteraad." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/adresstatus> ;
    skos:prefLabel "gehistoreerd" ;
    skos:relatedMatch <https://inspire.ec.europa.eu/codelist/StatusValue/retired> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/adresstatus> .

<https://data.vlaanderen.be/id/concept/adresstatus/inGebruik>
    a skos:Concept ;
    skos:definition "Het adres werd formeel goedgekeurd door de gemeenteraad en/of wordt actief gebruikt voor adressering." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/adresstatus> ;
    skos:prefLabel "in gebruik" ;
    skos:relatedMatch <https://inspire.ec.europa.eu/codelist/StatusValue/current> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/adresstatus> .

<https://data.vlaanderen.be/id/concept/adresstatus/voorgesteld>
    a skos:Concept ;
    skos:definition "Het adres werd voorgesteld maar nog niet goedgekeurd door de gemeenteraad." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/adresstatus> ;
    skos:prefLabel "voorgesteld" ;
    skos:relatedMatch <https://inspire.ec.europa.eu/codelist/StatusValue/proposed> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/adresstatus> .

<https://data.vlaanderen.be/id/concept/adresstatus/afgekeurd>
    a skos:Concept ;
    skos:definition "Wanneer het adres werd ingevoerd nog vóór er een vergunning met voorstel tot adressering werd verleend door de gemeenteraad en uiteindelijk besluit de gemeenteraad dit adres niet goed te keuren, dan wordt dit adres door de beheerder afgekeurd." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/adresstatus> ;
    skos:prefLabel "afgekeurd" ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/adresstatus> .

